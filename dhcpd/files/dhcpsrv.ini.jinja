[SETTINGS]
IPPOOL_1=192.168.0.2-254
IPBIND_1=192.168.0.1
AssociateBindsToPools=1
Trace=1
DeleteOnRelease=1
ExpiredLeaseTimeout=3600

[GENERAL]
LEASETIME=86400
NODETYPE=8
SUBNETMASK=255.255.255.0
NEXTSERVER=192.168.0.1
{% for dns_server in dns_servers -%}
DNS_{{ loop.index }}={{ dns_server }}
{% endfor -%}
ROUTER_0=192.168.0.1
[DNS-SETTINGS]
EnableDNS=1
FORWARD=1.1.1.1

[TFTP-SETTINGS]
EnableTFTP=0
ROOT=C:\tools\dhcpd\wwwroot
WritePermission=0

[HTTP-SETTINGS]
EnableHTTP=0
ROOT=C:\tools\dhcpd\wwwroot